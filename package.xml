<?xml version="1.0"?>
<package format="3">
  <name>tensorflow_ros_cpp</name>
  <version>3.1.2</version>
  <description>Catkin-friendly C++ bindings for the tensorflow package.</description>

  <maintainer email="martin.pecka@cvut.cz">Martin Pecka</maintainer>

  <license>BSD</license>

  <url type="website">https://wiki.ros.org/tensorflow_ros_cpp</url>
  <url type="repository">https://github.com/tradr-project/tensorflow_ros_cpp</url>
  <url type="bugtracker">https://github.com/tradr-project/tensorflow_ros_cpp/issues</url>

  <buildtool_depend>catkin</buildtool_depend>

  <build_depend condition="$ROS_PYTHON_VERSION == 2">python</build_depend>
  <build_depend condition="$ROS_PYTHON_VERSION != 2">python3</build_depend>
  <build_export_depend condition="$ROS_PYTHON_VERSION == 2">python</build_export_depend>
  <build_export_depend condition="$ROS_PYTHON_VERSION != 2">python3</build_export_depend>

  <build_depend condition="$ROS_PYTHON_VERSION == 2">python-pip</build_depend> <!-- optional -->
  <build_depend condition="$ROS_PYTHON_VERSION != 2">python3-pip</build_depend> <!-- optional -->

  <depend>python-tensorflow-pip</depend> <!-- optional -->
  <depend>tensorflow_catkin</depend> <!-- optional -->
</package>
